{
    "name": "ispeakerreact",
    "author": "yell0wsuit",
    "description": "An English-learning interactive tool written in React, designed to help learners practice speaking and listening",
    "private": true,
    "version": "3.2.1",
    "type": "module",
    "main": "main.cjs",
    "license": "Apache-2.0",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "lint": "eslint .",
        "preview": "vite preview",
        "start": "concurrently \"cross-env NODE_ENV=development vite\" \"wait-on http://localhost:5173 && cross-env NODE_ENV=development electron .\"",
        "package": "vite build --mode electron && electron-forge package",
        "make": "vite build --mode electron && electron-forge make",
        "vitebuildcli": "vite build --mode electron",
        "makecli": "electron-forge make",
        "appx": "vite build --mode electron && electron-forge package && cd ./out && electron-windows-store --input-directory ./iSpeakerReact-win32-x64 --output-directory ./ispeakerreact-appx --package-version ${npm_package_version}.0 --package-name ispeakerreact  --assets ../public/images/icons/windows11 -m ./appxmanifest.xml",
        "appxarm": "vite build --mode electron && electron-forge package --arch=arm64 && cd ./out && electron-windows-store --input-directory ./iSpeakerReact-win32-arm64 --output-directory ./ispeakerreact-appx --package-version ${npm_package_version}.0 --package-name ispeakerreact  --assets ../public/images/icons/windows11 -m ./appxmanifestARM.xml"
    },
    "dependencies": {
        "@dnd-kit/core": "^6.3.1",
        "@dnd-kit/sortable": "^10.0.0",
        "@vidstack/react": "^1.12.12",
        "@wavesurfer/react": "^1.0.8",
        "cors": "^2.8.5",
        "dexie": "^4.0.10",
        "electron-log": "^5.2.4",
        "electron-squirrel-startup": "^1.0.1",
        "express": "^4.21.2",
        "he": "^1.2.0",
        "i18next": "^24.2.1",
        "i18next-browser-languagedetector": "^8.0.2",
        "i18next-http-backend": "^3.0.1",
        "lodash": "^4.17.21",
        "masonry-layout": "^4.2.2",
        "mime": "^4.0.6",
        "nprogress": "^0.2.0",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-flip-toolkit": "^7.2.4",
        "react-i18next": "^15.4.0",
        "react-icons": "^5.4.0",
        "react-loading-skeleton": "^3.5.0",
        "react-router-dom": "^7.1.1",
        "sonner": "^1.7.1",
        "wavesurfer.js": "^7.8.15",
        "express-rate-limit": "^7.5.0"
    },
    "devDependencies": {
        "@electron-forge/cli": "^7.6.0",
        "@electron-forge/maker-deb": "^7.6.0",
        "@electron-forge/maker-dmg": "^7.6.0",
        "@electron-forge/maker-rpm": "^7.6.0",
        "@electron-forge/maker-squirrel": "^7.6.0",
        "@electron-forge/maker-zip": "^7.6.0",
        "@electron-forge/plugin-auto-unpack-natives": "^7.6.0",
        "@electron-forge/plugin-fuses": "^7.6.0",
        "@electron/fuses": "^1.8.0",
        "@eslint/js": "^9.17.0",
        "@tailwindcss/typography": "^0.5.16",
        "@types/react": "^19.0.4",
        "@types/react-dom": "^19.0.2",
        "@vitejs/plugin-react": "^4.3.4",
        "autoprefixer": "^10.4.20",
        "concurrently": "^9.1.2",
        "cross-env": "^7.0.3",
        "daisyui": "^4.12.23",
        "electron": "^33.3.1",
        "eslint": "^9.17.0",
        "eslint-plugin-react": "^7.37.3",
        "eslint-plugin-react-hooks": "^5.1.0",
        "eslint-plugin-react-refresh": "^0.4.16",
        "globals": "^15.14.0",
        "postcss": "^8.4.49",
        "prettier": "^3.4.2",
        "prettier-plugin-tailwindcss": "^0.6.9",
        "rollup-plugin-visualizer": "^5.14.0",
        "tailwindcss": "^3.4.17",
        "vite": "^6.0.7",
        "vite-plugin-pwa": "^0.21.1",
        "wait-on": "^8.0.2"
    },
    "optionalDependencies": {
        "appdmg": "latest"
    }
}
